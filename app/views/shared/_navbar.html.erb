<div class="navbar-cm">
  <div class="navbar-cm-left">
    <%= link_to root_path do %>
      <%= image_tag "logo.png", class: "navbar-cm-logo" %>
    <% end %>
  </div>
  <div class="navbar-cm-middle">
    <%= link_to root_path do %>
      <h3>Cocktail Memories</h3>
    <% end %>
  </div>
  <div class="navbar-cm-right">
    <%= form_tag search_path, method: :get do %>
      <%= text_field_tag :query, params[:query], class: "form-control", placeholder: "Find a movie" %>
    <% end %>
  </div>
</div>
